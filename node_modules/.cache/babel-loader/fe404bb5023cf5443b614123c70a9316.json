;AAgCA,EAAA,QAAA,oBACA,UAAA,CAAA;AACA,IAAA,KAAA,EAAA;AADA,GAAA,CADA,CAhCA;AAqCA,EAAA,KAAA,EAAA;AACA,IAAA,aAAA,EAAA,uBAAA,GAAA,EAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,kBAAA,EAAA;AACA,QAAA,YAAA,EAAA,CADA;AAEA,QAAA,SAAA,EAAA,CAFA;AAGA,QAAA,aAAA,EAAA;AAHA,OAAA;AAKA;AAPA;AArCA,CAAA","sourcesContent":["<template>\n    <el-card class=\"box-card\">\n        <div slot=\"header\" class=\"clearfix\">\n            <span>User List</span>\n            <v-btn style=\"float: right;\" small rounded tile outlined :color=\"color.addButton\" :to=\"{name: 'users.create'}\">\n                <v-icon left>mdi-plus-circle-outline</v-icon> Create\n            </v-btn>\n        </div>\n        <el-table :data=\"users\" style=\"width: 100%\" v-loading=\"$store.state.pageLoading\">\n            <el-table-column\n                    type=\"index\"\n                    width=\"50\">\n            </el-table-column>\n            <el-table-column\n                    label=\"Name\"\n                    prop=\"first_name\">\n            </el-table-column>\n            <el-table-column\n                    label=\"Email\"\n                    prop=\"email\">\n            </el-table-column>\n            <el-table-column\n                    label=\"Phone\"\n                    prop=\"phone_no\">\n            </el-table-column>\n            <el-table-column\n                    label=\"Country\"\n                    prop=\"country.name\">\n            </el-table-column>\n            <el-table-column align=\"right\">\n                <template slot=\"header\" slot-scope=\"scope\">\n                    <el-input\n                            v-model=\"searchKeyword\"\n                            size=\"mini\"\n                            placeholder=\"Type to search\"></el-input>\n                </template>\n                <template slot-scope=\"scope\">\n                    <v-btn class=\"mx-2\" dark small :color=\"color.editButton\" :to=\"{name: 'users.edit', params: {id: scope.row.id}}\">\n                        <v-icon dark small>edit</v-icon>\n                    </v-btn>\n                    <v-btn class=\"mx-2\" dark small :color=\"color.deleteButton\" @click=\"deletePopup(scope.row)\">\n                        <v-icon dark small>delete</v-icon>\n                    </v-btn>\n                    <div v-if=\"$store.state.deleteDialog\">\n                        <DeletePopup :item=\"item\" action=\"ACT_DELETE_USER\" />\n                    </div>\n                </template>\n            </el-table-column>\n        </el-table>\n        <Pagination :action=\"searchKeyword ? 'ACT_SEARCH_USERS' : 'ACT_ALL_USERS'\" :query=\"searchKeyword\" />\n        <v-snackbar v-model=\"$store.state.snackbar\" left>\n            {{ $store.state.notificationMessage }}\n        </v-snackbar>\n    </el-card>\n</template>\n\n<script>\n    import { mapGetters } from 'vuex';\n    import color from '../../plugins/colors';\n    import Pagination from \"../../components/default/Pagination\";\n    import DeletePopup from \"../../components/default/DeletePopup\";\n\n    export default {\n        name: \"Index\",\n        metaInfo: {\n            titleTemplate: 'User | Dashboard | Faton',\n        },\n        components: {\n            Pagination,\n            DeletePopup,\n        },\n        data() {\n            return {\n                color: color,\n                searchKeyword: '',\n                item: {\n                    profile: {}\n                },\n                formLabelWidth: '90px',\n                dialogTitle: ''\n            }\n        },\n        created() {\n            this.$store.dispatch('ACT_ALL_USERS', {\n                current_page: 1,\n                next_page: 1\n            });\n        },\n        methods: {\n            deletePopup(row) {\n                this.item = row;\n                this.$store.state.deleteDialog = true;\n            }\n        },\n        computed: {\n            ...mapGetters({\n                users : 'GET_ALL_USER_DATA',\n            })\n        },\n        watch: {\n            searchKeyword: function(val) {\n                this.$store.dispatch('ACT_SEARCH_USERS', {\n                    current_page: 1,\n                    next_page: 1,\n                    searchKeyword: val\n                });\n            }\n        }\n    }\n</script>\n\n<style>\n    .el-dialog__body {\n        padding-bottom: 0px !important;\n    }\n</style>"],"sourceRoot":"src/views/user"}]}