  <el-table :data=\"chapters\" style=\"width: 100%\" v-loading=\"$store.state.pageLoading\">\n            <el-table-column\n                    type=\"index\"\n                    width=\"50\">\n            </el-table-column>\n            <el-table-column\n                    label=\"Name\"\n                    prop=\"name\">\n            </el-table-column>\n            <el-table-column\n                    label=\"Code\"\n                    prop=\"code\">\n            </el-table-column>\n            <el-table-column align=\"right\">\n                <template slot=\"header\" slot-scope=\"scope\">\n                    <el-input\n                            v-model=\"searchKeyword\"\n                            size=\"mini\"\n                            placeholder=\"Type to search\"></el-input>\n                </template>\n                <template slot-scope=\"scope\">\n                    <v-btn class=\"mx-2\" dark small :color=\"color.editButton\" @click=\"editItem(scope.row)\">\n                        <v-icon dark small>edit</v-icon>\n                    </v-btn>\n                    <v-btn class=\"mx-2\" dark small :color=\"color.deleteButton\" @click=\"deletePopup(scope.row)\">\n                        <v-icon dark small>delete</v-icon>\n                    </v-btn>\n                    <div v-if=\"$store.state.deleteDialog\">\n                        <DeletePopup :item=\"item\" action=\"ACT_DELETE_LANGUAGE\" />\n                    </div>\n                </template>\n            </el-table-column>\n        </el-table>\n        <Pagination :action=\"searchKeyword ? 'ACT_SEARCH_LANGUAGES' : 'ACT_ALL_LANGUAGES'\" :query=\"searchKeyword\" />\n        <!--Create Form-->\n        <el-dialog\n                title=\"Create Language\"\n                :visible.sync=\"$store.state.createDialogVisible\"\n                width=\"30%\">\n                <el-form :model=\"item\">\n                    <el-form-item label=\"Promotion name\" :label-width=\"formLabelWidth\">\n                        <el-input v-model=\"item.name\" autocomplete=\"off\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"Zones\" :label-width=\"formLabelWidth\">\n                        <el-input v-model=\"item.code\" autocomplete=\"off\"></el-input>\n                    </el-form-item>\n                </el-form>\n                <span slot=\"footer\" class=\"dialog-footer\">\n                    <el-button type=\"primary\">Submit</el-button>\n                </span>\n        </el-dialog>\n        <!--End Create Form-->\n        <!--Edit Form-->\n        <el-dialog\n                title=\"Create Language\"\n                :visible.sync=\"$store.state.editDialogVisible\"\n                width=\"30%\">\n            <el-form :model=\"item\">\n                <el-form-item label=\"Promotion name\" :label-width=\"formLabelWidth\">\n                    <el-input v-model=\"item.name\" autocomplete=\"off\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"Zones\" :label-width=\"formLabelWidth\">\n                    <el-input v-model=\"item.code\" autocomplete=\"off\"></el-input>\n                </el-form-item>\n            </el-form>\n            <span slot=\"footer\" class=\"dialog-footer\">\n                    <el-button type=\"primary\">Submit</el-button>\n            </span>\n        </el-dialog>\n        <!--End Edit Form-->\n    </el-card>\n</template>\n\n<script>\n    import { mapGetters } from 'vuex';\n    import color from '../../plugins/colors';\n    import Pagination from \"../../components/default/Pagination\";\n    import DeletePopup from \"../../components/default/DeletePopup\";\n    import Create from \"../../components/language/Create\";\n    import Edit from \"../../components/language/Edit\";\n\n    export default {\n        name: \"Index\",\n        metaInfo: {\n            titleTemplate: 'Language | Dashboard | Faton',\n        },\n        components: {\n            Pagination,\n            DeletePopup,\n            Create,\n            Edit\n        },\n        data() {\n            return {\n                color: color,\n                searchKeyword: '',\n                item: {},\n                formLabelWidth: '120px'\n            }\n        },\n        created() {\n            this.$store.dispatch('ACT_ALL_LANGUAGES', {\n                current_page: 1,\n                next_page: 1\n            });\n        },\n        methods: {\n            deletePopup(row) {\n                this.item = row;\n                this.$store.state.deleteDialog = true;\n            },\n            createItem() {\n                this.$store.state.createDialogVisible = true;\n            },\n            editItem(row) {\n                this.item = row;\n                this.$store.state.editDialogVisible = true;\n            }\n        },\n        computed: {\n            ...mapGetters({\n                chapters : 'GET_ALL_LANGUAGE_DATA'\n            })\n        },\n        watch: {\n            searchKeyword: function(val) {\n                this.$store.dispatch('ACT_SEARCH_LANGUAGES', {\n                    current_page: 1,\n                    next_page: 1,\n                    searchKeyword: val\n                });\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .el-dialog__body {\n        padding-bottom: 0px !important;\n    }\n</style>"]}]}