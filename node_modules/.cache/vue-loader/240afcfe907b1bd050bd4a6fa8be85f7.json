        <p>{{ item.keywords }}</p>\n            </el-col>\n        </el-row>\n        <hr>\n        <el-row :gutter=\"20\" class=\"pt-2\">\n            <el-col :span=\"12\">\n                <h3>Description</h3>\n                <p>{{ item.description }}</p>\n            </el-col>\n            <el-col :span=\"12\">\n                <h3>Meta Description</h3>\n                <p>{{ item.meta_description }}</p>\n            </el-col>\n        </el-row>\n\n        <div v-for=\"(translation, index) in item.translations\">\n            <hr>\n            <h3 class=\"pt-2 pb-1\">{{ translation.language.name }} ({{ translation.language.code }})</h3>\n            <el-row :gutter=\"20\">\n                <el-col :span=\"6\">\n                    <p>{{ translation.value_1 }}</p>\n                </el-col>\n                <el-col :span=\"18\">\n                    <p>{{ translation.value_2 }}</p>\n                </el-col>\n            </el-row>\n        </div>\n\n        <el-row :gutter=\"20\">\n            <el-col :span=\"8\">\n                <h3>Chapter Image</h3>\n                <v-img :src=\"item.image\" aspect-ratio=\"2\"></v-img>\n            </el-col>\n        </el-row>\n    </el-dialog>\n</template>\n\n<script>\n    export default {\n        name: \"Details\",\n        props: ['item']\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}